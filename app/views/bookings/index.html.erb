<p id="notice"><%= notice %></p>


<h1>Bookings</h1>

<table>
  <thead>
  <tr>
    <th>UserID</th>
    <th>RoomNo</th>
    <th>In Time</th>
    <th>Status</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.userid %></td>
        <td><%= booking.room_no %></td>
        <td><%= booking.intime %></td>
        <td><%= booking.booking_status %></td>
        <td><%= link_to 'Show', booking %></td>

      </tr>
  <% end %>
  </tbody>
</table>

<h1>Deleted Bookings</h1>

<table>
  <thead>
  <tr>
    <th>UserID</th>
    <th>RoomNo</th>
    <th>In Time</th>

    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @deleted.each do |booking| %>
      <tr>
        <td><%= booking.userid %></td>
        <td><%= booking.room_no %></td>
        <td><%= booking.intime %></td>



      </tr>
  <% end %>
  </tbody>
</table>




