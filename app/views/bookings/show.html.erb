<p id="notice"><%= notice %></p>

<p>
  <strong>Userid:</strong>
  <%= @booking.userid %>
</p>

<p>
  <strong>Room no:</strong>
  <%= @booking.room_no %>
</p>

<p>
  <strong>Intime:</strong>
  <%= @booking.intime.strftime('%m/%d/%Y %H:%M') %>
</p>



<%= link_to 'Edit', edit_booking_path(@booking) %> |
<%= link_to 'Back', "/bookings/search_static" %>
<% if  DateTime.parse(@booking.intime.strftime('%m/%d/%Y %H:%M')) > DateTime.parse(Time.now.strftime('%m/%d/%Y %H:%M')) %>
    <button onclick="showBookingMailPopup(<%= @booking.id %>)"> Send email </button>
<% end%>
